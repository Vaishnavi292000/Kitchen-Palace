{
  "type": "APL",
  "version": "2023.1",
  "description": "This APL document places text on the screen. Has Previous and next on voice commands",
  "theme": "dark",
  "import": [
    {
      "name": "alexa-layouts",
      "version": "1.7.0"
    }
  ],
  "mainTemplate": {
    "parameters": [
      "payload"
    ],
    "items": [
      {
        "type": "Container",
        "height": "100vh",
        "width": "100vw",
        "items": [
            {
                "type": "AlexaBackground",
                "backgroundImageSource": "data:image/jpeg;base64,",
                "colorOverlay": true
            },
            {
                "type": "Text",
                "id": "helloTextComponent",
                "textAlign": "center",
                "textAlignVertical": "center",
                "paddingLeft": "@spacingSmall",
                "paddingRight": "@spacingSmall",
                "paddingTop": "@spacingXLarge",
                "fontSize": "@fontSizeXLarge",
                "style": "textStyleBody",
                "fontFamily": "times new roman",
                "text": "Welcome to Kitchen Palace!!",
                "maxLines": 4,
                "grow": 1
            },
            {
              "type": "AlexaFooter",
              "hintText": "Say, <b>Show my recipes</b>!!"
            }
        ]
      }
    ]
  }
}